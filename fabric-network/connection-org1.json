{
    "name": "car-registry-network",
    "version": "1.0.0",
    "client": {
        "organization": "Org1",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "Org1": {
            "mspid": "Org1MSP",
            "peers": [
                "peer0.example.com"
            ],
            "certificateAuthorities": [
                "ca.example.com"
            ]
        }
    },
    "orderers": {
        "orderer": {
            "url": "grpcs://orderer:7050",
            "tlsCACerts": {
                "path": "../crypto-config/ordererOrganizations/example.com/orderers/orderer/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "orderer",
                "hostnameOverride": "orderer"
            }
        }
    },
    "peers": {
        "peer0.example.com": {
            "url": "grpcs://peer0.example.com:7051",
            "tlsCACerts": {
                "path": "../crypto-config/peerOrganizations/example.com/peers/peer0.example.com/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.example.com",
                "hostnameOverride": "peer0.example.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.example.com": {
            "url": "https://localhost:7054",
            "caName": "ca-org1",
            "httpOptions": {
                "verify": false
            }
        }
   }
}
